#!/bin/bash

imports="
import scala.language.postfixOps
import java.io.File
import scutil.lang._
import scutil.implicits._
import scutil.io.Files._
import scutil.io.Charsets.utf_8
import scutil.io.Base64
import scutil.io.URIComponent
import scutil.text.Human
import scutil.time._
"

initial="$(echo "$imports" | sed 's/$/\\n/' | tr -d '\n')"

sbt ";project scutil-core;set initialCommands in console := \"$initial\";console"
